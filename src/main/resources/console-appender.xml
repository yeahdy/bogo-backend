<included>
    <springProfile name="console-logging">
        <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
            <encoder>
                <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS, ${logback.timezone:-Asia/Seoul}} [%thread] %-5level %logger{36} - %msg%n
                </pattern>
            </encoder>
        </appender>
    </springProfile>

</included>